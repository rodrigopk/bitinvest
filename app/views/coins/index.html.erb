<% provide(:title, t(:currencies_link)) %>
<h1><%= t(:currencies_link) %></h1>

<%= will_paginate %>
<table class="table">
  <%= render partial: 'table_header'%>
  <tbody>
    <% @coins.each do |coin| %>
      <% if !coin.is_fiat?  %>
        <%= render coin %>
      <% end  %>  
    <% end %>
  </tbody>
</table>
<%= will_paginate %>


